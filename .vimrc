" My Own vimrc
set nocompatible

filetype on

filetype plugin on

filetype indent on

syntax on

set number

set cursorline

" set cursorcolumn

set tabstop=4

set expandtab

set nobackup

set ignorecase

set showmode

set showmatch

set hlsearch

set history=1000

set wildmenu

set wildmode=list:longest

set wildignore=*.docx,*.jpg,*.png,*.gif,*.pdf,*.pyc,*.exe,*.flv,*.img,*.xlsx
